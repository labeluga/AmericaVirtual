@model IEnumerable<Barberia.Models.CajaListaModel>
@using Resources

@if (Model.Count() != 0)
{
    <table id="cajasLista" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Editar</th>
                <th>Número</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <a style="padding:15px" title="@Resources.Editar" href="@Url.Action("Edit", new { id = item.ORID })"><i class="fas fa-pen"></i></a>
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Numero)
                    </td>

                    
                </tr>
            }
        </tbody>
        <tfoot>
            <tr>
                <th>Editar</th>
                <th>Número</th>

            </tr>
        </tfoot>
    </table>
}
